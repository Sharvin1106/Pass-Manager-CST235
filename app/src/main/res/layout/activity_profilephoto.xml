<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".profilephoto"
    android:background="@color/black">


    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:layout_gravity="center_horizontal"
        android:layout_weight="1"
        android:foregroundGravity="center"
        android:gravity="center"
        android:text="Choose An Avatar"
        android:textColor="@color/white"
        android:textSize="30sp"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        >


        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="165dp"
            android:layout_height="140dp"
            android:layout_weight="1"
            android:onClick="send2"
            android:adjustViewBounds="true"
            android:layout_margin="10dp"
            app:srcCompat="@drawable/facebooklogo" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="113dp"
            android:layout_height="match_parent"
            android:layout_margin="10dp"
            android:layout_weight="1"
            android:adjustViewBounds="true"
            android:onClick="send"
            app:srcCompat="@drawable/instagramlogo" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="165dp"
            android:layout_height="140dp"
            android:layout_weight="1"
            android:onClick="send3"
            android:layout_margin="10dp"
            android:adjustViewBounds="true"
            app:srcCompat="@drawable/whatsapp" />

    </LinearLayout>


    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="134dp"
            android:layout_height="102dp"
            android:layout_margin="10dp"
            android:layout_weight="1"
            android:adjustViewBounds="true"
            android:onClick="send4"
            app:srcCompat="@drawable/tinder" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="134dp"
            android:layout_height="102dp"
            android:layout_margin="10dp"
            android:layout_weight="1"
            android:adjustViewBounds="true"
            android:onClick="send5"
            app:srcCompat="@drawable/tiktok" />

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="134dp"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_weight="1"
            android:adjustViewBounds="true"
            android:onClick="send6"
            app:srcCompat="@drawable/twitterlogo" />


    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="129dp"
            android:layout_height="94dp"
            android:layout_margin="10dp"
            android:layout_weight="1"
            android:onClick="send7"
            app:srcCompat="@drawable/telegramlogo" />

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="80dp"
            android:layout_height="99dp"
            android:layout_weight="1"
            android:layout_margin="10dp"
            android:onClick="send8"
            app:srcCompat="@drawable/shoppinglogo" />

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_margin="10dp"
            android:onClick="send9"
            app:srcCompat="@drawable/bookslogo" />


    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="230dp"
        android:background="@color/white">

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="131dp"
            android:layout_height="98dp"
            android:layout_margin="10dp"
            android:layout_weight="1"
            android:onClick="send10"
            app:srcCompat="@drawable/game" />

        <ImageView
            android:id="@+id/imageView20"
            android:layout_width="103dp"
            android:layout_height="99dp"
            android:layout_margin="10dp"
            android:layout_weight="1"
            android:onClick="send11"
            app:srcCompat="@drawable/browser" />

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="100dp"
            android:layout_height="99dp"
            android:layout_margin="10dp"
            android:layout_weight="1"
            android:onClick="send12"
            app:srcCompat="@drawable/bankcard" />


    </LinearLayout>



</LinearLayout>